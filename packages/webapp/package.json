{
  "name": "@cbosuite/webapp",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "essex clean .version dist/ out/ node_modules/.cache/ public/localizations public/workers",
    "start": "yarn write_version && NODE_CONFIG_ENV=development vite -c vite.config.js",
    "start:webapp": "yarn start",
    "start:webapp:static": "http-server dist -p 3000",
    "typecheck": "tsc -b .",
    "write_version": "node scripts/writeVersion",
    "copy_404": "shx cp dist/index.html dist/404.html",
    "build_base": "",
    "build": "NODE_CONFIG_ENV=ci yarn build:webapp",
    "build:webapp": "run-s clean typecheck assets write_version vite_build copy_404",
    "deploy:webapp": "essex deploy --type azure-blob-storage --storageAccount $WEBAPP_STORAGE_ACCOUNT --storageAccountKey $WEBAPP_STORAGE_KEY --verbose --paths dist/",
    "vite_build": "vite build -c vite.config.js",
    "plop": "plop",
    "plop:webapp": "plop",
    "assets": "run-p assets:localizations assets:serviceworkers",
    "assets:localizations": "node scripts/generateLocalizationFiles.js",
    "assets:serviceworkers": "node scripts/generateServiceWorkers.js",
    "test:localizations": "node scripts/lintLocalizationFiles.js",
    "test": "run-s test:webapp:coverage test:localizations",
    "test:webapp": "DEBUG=\"cbosuite:*\" jest --verbose --runInBand",
    "test:webapp:coverage": "yarn test:webapp --coverage",
    "test:webapp:watch": "yarn test:webapp --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@cbosuite/schema": "workspace:^",
    "@fluentui/merge-styles": "^8.4.0",
    "@fluentui/react": "^8.58.0",
    "@fluentui/react-hooks": "^8.5.0",
    "@microsoft/applicationinsights-react-js": "^3.2.4",
    "@microsoft/applicationinsights-web": "^2.7.4",
    "apollo3-cache-persist": "^0.14.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "config": "^3.3.7",
    "core-js": "^3.21.1",
    "crypto-js": "^4.1.1",
    "debug": "^4.3.3",
    "firebase": "^8.10.1",
    "formik": "^2.2.9",
    "graphql": "^16.4.0",
    "history": "^4.10.1",
    "json2csv": "^5.0.7",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "print-js": "^1.6.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-dom-confetti": "^0.2.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.24.6",
    "react-is": "^17.0.2",
    "react-paginated-list": "^1.1.5",
    "react-router-dom": "^5.3.0",
    "react-select": "^4.3.1",
    "react-stores": "^5.5.0",
    "react-toast-notifications": "^2.5.1",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.6.1",
    "recoil-persist": "^3.0.0",
    "redbox-react": "^1.6.0",
    "styled-components": "^5.3.3",
    "subscriptions-transport-ws": "^0.11.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@essex/jest-config": "^21.0.4",
    "@essex/scripts": "^20.4.6",
    "@essex/vite-config": "^19.0.4",
    "@testing-library/react": "^12.1.3",
    "@tsconfig/node14": "^1.0.1",
    "@types/babel__core": "^7.1.18",
    "@types/babel__preset-env": "^7.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.41",
    "@types/crypto-js": "^4.1.1",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@types/react-is": "^17.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^4.0.18",
