{
  "name": "@cbosuite/webapp",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "essex clean dist/ out/ .next node_modules/.cache/ public/localizations public/workers",
    "start": "yarn assets && NODE_CONFIG_ENV=development next dev",
    "start:webapp": "yarn start",
    "build": "run-s clean assets next_build next_export",
    "build:webapp:integ": "NODE_CONFIG_ENV=integration yarn build",
    "build:webapp:prod": "NODE_CONFIG_ENV=production yarn build",
    "build:webapp:demo": "NODE_CONFIG_ENV=demo yarn build",
    "deploy:webapp": "essex deploy --type azure-blob-storage --storageAccount $WEBAPP_STORAGE_ACCOUNT --storageAccountKey $WEBAPP_STORAGE_KEY --verbose --paths out/",
    "next_build": "next build",
    "next_export": "next export",
    "plop": "plop",
    "plop:webapp": "plop",
    "assets": "run-p assets:localizations assets:serviceworkers",
    "assets:localizations": "node scripts/generateLocalizationFiles",
    "assets:serviceworkers": "node scripts/generateServiceWorkers",
    "lint:localizations": "node scripts/lintLocalizationFiles",
    "test": "yarn assets && jest && yarn lint:localizations"
  },
  "dependencies": {
    "@apollo/client": "3.4.9",
    "@cbosuite/schema": "workspace:packages/schema",
    "@fluentui/react": "^8.34.6",
    "@fluentui/react-hooks": "^8.3.2",
    "@microsoft/applicationinsights-react-js": "^3.2.0",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "apollo3-cache-persist": "^0.13.0",
    "bootstrap": "^5.1.0",
    "classnames": "^2.3.1",
    "config": "^3.3.6",
    "core-js": "^3.17.2",
    "debug": "^4.3.2",
    "firebase": "^8.10.0",
    "formik": "^2.2.9",
    "graphql": "^15.6.0",
    "json2csv": "^5.0.6",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-dom": "^17.0.2",
    "react-dom-confetti": "^0.2.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.20.10",
    "react-is": "^17.0.2",
    "react-paginated-list": "^1.1.5",
    "react-select": "^4.3.1",
    "react-toast-notifications": "^2.5.1",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.4.1",
    "recoil-persist": "^3.0.0",
    "redbox-react": "^1.6.0",
    "styled-components": "^5.3.1",
    "subscriptions-transport-ws": "^0.9.19",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@essex/jest-config": "^18.0.1",
    "@essex/scripts": "^18.2.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/config": "^0.0.39",
    "@types/debug": "^4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^14.17.18",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6",
    "@types/react-is": "^17.0.2",
    "@types/react-select": "^4.0.18",
    "@types/react-transition-group": "^4.4.3",
    "@types/styled-components": "^5.1.14",
    "@types/yup": "^0.29.13",
    "autoprefixer": "^10.3.5",
    "babel-plugin-transform-define": "^1.3.2",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.1",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "postcss": "^8.3.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "1.42.1",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.4.3"
  }
}
